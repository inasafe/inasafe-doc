msgstr ""

# b2ae0081205f455cb8c9619666930b05
#: ../../source/training/osm/Chapter-02-getting-started.rst:15
msgid "Know how to navigate OpenStreetMap (search, move, zoom in/zoom out)"
msgstr ""

# df1a745d1e874edf8dbb4f6b1ca24ef7
